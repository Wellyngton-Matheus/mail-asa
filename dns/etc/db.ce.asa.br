; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ce.asa.br. root.ce.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ce.asa.br.
@	IN	NS	ns1.ce.asa.br.
@	IN	MX	10 mail.ce.asa.br.
@	IN	A	192.168.0.10

ns1	IN	A	192.168.0.10
mail	IN	A	192.168.0.10
