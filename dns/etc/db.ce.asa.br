;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ce.asa.br. root.ce.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.ce.asa.br.
@	IN	NS	ns1.ce.asa.br.
;@	IN	NS	ns2.ce.asa.br.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1			A	192.168.0.10
mail		A 	192.168.0.10


